{
  "name": "rynk-mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "description": "Official Rynk mobile application built with Expo and React Native.",
  "author": "Rynk Team",
  "license": "MIT",
  "homepage": "https://rynk.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/rynk-ai/rynk-mobile.git"
  },
  "bugs": {
    "url": "https://github.com/rynk-ai/rynk-mobile/issues"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:clean": "expo prebuild --clean && expo run:android",
    "ios": "expo run:ios",
    "ios:clean": "expo prebuild --clean && expo run:ios",
    "both": "expo run:ios & expo run:android",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "run:ios": "expo run:ios",
    "run:android": "expo run:android",
    "run:ios-device": "expo run:ios --device",
    "run:android-device": "expo run:android --device",
    "deploy": "eas build --platform all --profile production --auto-submit",
    "push": "eas update --branch production",
    "deploy:local": "./deploy-local.sh",
    "deploy:ios": "./deploy-local.sh i",
    "deploy:android": "./deploy-local.sh a"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
